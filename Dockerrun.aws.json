{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "060712730170.dkr.ecr.eu-west-1.amazonaws.com/multi-client",
            "hostname": "client",
            "essential": false
        },
        {
            "name": "server",
            "image": "060712730170.dkr.ecr.eu-west-1.amazonaws.com/multi-server",
            "hostname": "api",
            "essential": false
        },
        {
            "name": "worker",
            "image:": "060712730170.dkr.ecr.eu-west-1.amazonaws.com/multi-worker",
            "hostname": "worker",
            "essential": false
        },
        {
            "name": "nginx",
            "image": "060712730170.dkr.ecr.eu-west-1.amazonaws.com/multi-nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": [
                "client", "server"
            ]
        }
    ]
}